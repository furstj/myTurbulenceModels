FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           volVectorField;
    object          U;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (5.4 0 0);

boundaryField
{
    inlet      
    {
        type            fixedValue;
        value           uniform (5.4 0 0);
    }

    outlet
    {
        type            zeroGradient;
    }

    lowerWall
    {
        type            fixedValue;
	value     uniform (0 0 0);
    }

    lowerSymmetry
    {
        type            slip;
    }

    upperSymmetry
    {
        type            slip;
    }

}


